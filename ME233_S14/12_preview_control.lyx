\begin_deeper
\begin_layout Standard
We consider controlling the system
\begin_inset Formula 
\begin{align}
x\left(k+1\right) & =Ax\left(k\right)+Bu\left(k\right)\label{eq:previewControl_sys}\\
y\left(k\right) & =Cx\left(k\right)\nonumber 
\end{align}

\end_inset

where
\begin_inset Formula 
\[
x\in\mathbb{R}^{n},\ u\in\mathbb{R}^{m},\ y\in\mathbb{R}^{r}
\]

\end_inset


\begin_inset Note Note
status open

\begin_layout Plain Layout
Objective: 
\begin_inset Formula $y\left(k\right)$
\end_inset

 follows 
\begin_inset Formula $y_{d}\left(k\right)$
\end_inset

 under a quadratic performance index.
\end_layout

\end_inset


\end_layout

\begin_layout Standard
Optimal tracking with full reference information (homework 1):
\begin_inset ERT
status open

\begin_layout Plain Layout

{
\backslash
footnotesize
\end_layout

\end_inset


\begin_inset Formula 
\begin{multline}
\min_{U_{0}}J:=\frac{1}{2}\left[y_{d}(N)-y(N)\right]^{T}S\left[y_{d}(N)-y(N)\right]\\
+\frac{1}{2}\sum_{k=0}^{N-1}\left(\left[y_{d}(k)-y(k)\right]^{T}Q_{y}\left[y_{d}(k)-y(k)\right]+u(k)^{T}Ru(k)\right)
\end{multline}

\end_inset


\begin_inset Formula 
\begin{eqnarray}
u^{o}(k) & = & -\left[R+B^{T}P(k+1)B\right]^{-1}B^{T}\left[P(k+1)Ax(k)+b^{T}(k+1)\right]\\
J_{k}^{o}\left(x(k)\right) & = & \frac{1}{2}x^{T}(k)P(k)x(k)+b(k)x(k)+c(k)
\end{eqnarray}

\end_inset


\end_layout

\begin_layout NoteItem
\begin_inset Formula 
\begin{eqnarray}
P(k) & = & C^{T}Q_{y}C+A^{T}P(k+1)A-A^{T}P(k+1)B\left[R+B^{T}P(k+1)B\right]^{-1}B^{T}P(k+1)A\\
b(k) & = & -y_{d}^{T}(k)Q_{y}C+b(k+1)\{A-B(R+B^{T}P(k+1)B)^{-1}B^{T}P(k+1)A\}\\
c(k) & = & c(k+1)+\frac{1}{2}y_{d}^{T}(k)Q_{y}y_{d}(k)-\frac{1}{2}b(k+1)B\left[R+B^{T}P(k+1)B\right]^{-1}B^{T}b(k+1)
\end{eqnarray}

\end_inset


